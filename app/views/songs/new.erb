<form action="/songs" method="POST">
  <label>Name:</label>
  <br></br>
  <input id="Name" type="text" name="song[name]" value="Name"></input>

  <br></br>
  <label>Choose a genre:<label>
  <br></br>

  <% @genres.each do |genre| %>
  <input type="checkbox" name="song[genre_ids][]" value="<%= genre.name %>"
  id="<%= genre.name %>"><%= genre.name %></input>
  <% end %>

  <br></br>
  <label>Artist Name</label>
  <input type="text" name="song[artist_name]" id="Artist Name"></input>
  <br></br>
  <input type="submit" value="Create">
</form>
