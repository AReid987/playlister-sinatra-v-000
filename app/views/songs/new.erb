<form action="/songs" method="POST">
  <label>Name:</label>
  <br></br>
  <input id="Name" type="text" name="song[name]" value="Name"></input>

  <br></br>
  <% @genres.each do |genre| %>
  <input type="checkbox" name="song[genre_ids]" value="<%= genre.id %>">
  <% end %>

  <br></br>
  <input type="submit" value="Create">
</form>
